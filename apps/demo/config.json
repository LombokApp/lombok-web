{
  "description": "The demo app.",
  "identifier": "demo",
  "label": "Demo App",
  "tasks": [
    {
      "identifier": "do_special_thing",
      "label": "Do Special Thing",
      "description": "Do special thing for each new object.",
      "triggers": [
        {
          "type": "event",
          "event": "platform:object_added",
          "inputParams": {
            "folderId": "event:folderId",
            "objectKey": "event:objectKey"
          }
        }
      ],
      "worker": "special_worker"
    }
  ],
  "workers": {
    "special_worker": {
      "description": "Manages special things for objects.",
      "environmentVariables": {
        "SPECIAL_THING_ENV_VAR": "<some_dummy_env_var_value>"
      }
    }
  },
  "ui": {
    "main": {
      "description": "The main UI for the demo app"
    }
  },
  "contributions": {
    "routes": [
      {
        "label": "Demo",
        "uiIdentifier": "main",
        "iconPath": "/assets/logo.png",
        "path": "/demo-page"
      },
      {
        "label": "Landing Page",
        "uiIdentifier": "main",
        "iconPath": "/assets/logo.png",
        "path": "/"
      },
      {
        "label": "Thing Doer",
        "uiIdentifier": "main",
        "iconPath": "/assets/logo.png",
        "path": "/thing-doer"
      }
    ],
    "sidebarMenuLinks": [
      {
        "label": "Landing Page",
        "iconPath": "/assets/logo.png",
        "uiIdentifier": "main",
        "path": "/"
      },
      {
        "label": "Demo Page",
        "iconPath": "/assets/logo.png",
        "uiIdentifier": "main",
        "path": "/demo-page"
      }
    ],
    "folderActionMenuLinks": [
      {
        "label": "Do Folder Thing",
        "iconPath": "/assets/logo.png",
        "uiIdentifier": "main",
        "path": "/demo-page?folderId={folderId}"
      }
    ],
    "objectActionMenuLinks": [
      {
        "label": "Do Object Thing",
        "iconPath": "/assets/logo.png",
        "uiIdentifier": "main",
        "path": "/demo-page?folderId={folderId}&objectKey={objectKey}"
      }
    ],
    "folderSidebarEmbeds": [
      {
        "label": "Demo Folder Sidebar Embed",
        "iconPath": "/assets/logo.png",
        "uiIdentifier": "main",
        "path": "/demo-page"
      }
    ],
    "objectSidebarEmbeds": [
      {
        "label": "Demo Object Sidebar Embed",
        "iconPath": "/assets/logo.png",
        "uiIdentifier": "main",
        "path": "/demo-page"
      }
    ]
  },
  "emittableEvents": []
}
