{
  "description": "The demo app.",
  "identifier": "demo",
  "label": "Demo App",
  "tasks": [
    {
      "identifier": "demo_object_added_worker_task",
      "label": "Demo Object Added Worker",
      "description": "A task that runs for every newly added object.",
      "triggers": ["platform:object_added"],
      "worker": "demo_object_added_worker"
    },
    {
      "identifier": "demo_scheduled_worker_task",
      "label": "Demo Scheduled Worker",
      "description": "A task that runs in response to a schedule event.",
      "triggers": ["platform:schedule:hourly"],
      "worker": "demo_scheduled_worker"
    }
  ],
  "workers": {
    "demo_object_added_worker": {
      "description": "Runs for every newly added object.",
      "environmentVariables": {
        "MY_CUSTOM_ENV_VAR": "_some_non_secret_env_var_value_"
      }
    },
    "demo_scheduled_worker": {
      "description": "Runs in response to a schedule event.",
      "environmentVariables": {
        "MY_CUSTOM_ENV_VAR": "_some_non_secret_env_var_value_"
      }
    },
    "demo_api_request_worker": {
      "description": "Is run to service and incoming API request.",
      "environmentVariables": {
        "MY_CUSTOM_ENV_VAR": "_some_non_secret_env_var_value_"
      }
    }
  },
  "ui": {
    "main": {
      "description": "The main UI for the demo app"
    }
  },
  "contributions": {
    "routes": {
      "base": {
        "uiIdentifier": "main",
        "path": "/"
      },
      "special": {
        "uiIdentifier": "main",
        "path": "/demo-page"
      }

    },
    "sidebarMenuLinks": [
      {
        "label": "Landing Page",
        "routeIdentifier": "base",
        "iconPath": "/assets/logo.png"
      }
    ],
    "folderActionMenuLinks": [
      {
        "label": "Do Folder Thing",
        "routeIdentifier": "base",
        "iconPath": "/assets/logo.png"
      }
    ],
    "objectActionMenuLinks": [
      {
        "label": "Do Object Thing",
        "routeIdentifier": "base",
        "iconPath": "/assets/logo.png"
      }
    ],
    "folderSidebarViews": [
      {
        "label": "Demo Folder Sidebar Embed",
        "routeIdentifier": "special",
        "iconPath": "/assets/logo.png"
      }
    ],
    "objectSidebarViews": [
      {
        "label": "Demo Object Sidebar Embed",
        "routeIdentifier": "base",
        "iconPath": "/assets/logo.png"
      }
    ],
    "objectDetailViews": [
      {
        "label": "Demo Object View Embed",
        "routeIdentifier": "special",
        "iconPath": "/assets/logo.png"
      }
    ]
  },
  "emittableEvents": []
}
