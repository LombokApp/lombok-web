{
  "name": "@stellariscloud/nestjsapi",
  "private": true,
  "scripts": {
    "dev": "bun --watch --bun src/main.ts",
    "nestdev": "nest start",
    "build": "nest build",
    "test": "jest",
    "reset:sdk": "(rm -rf ./node_modules/@stellariscloud/modules-worker-sdk || true)",
    "metadata:generate": "rm -rf ./src/nestjs-metadata.ts && npx ts-node src/generate-metadata.ts",
    "api-client:generate": "./cmd/generate-clients.sh",
    "prettier:check": "prettier --check \"./**/*.(ts|tsx)\"",
    "prettier:write": "prettier --write \"./**/*.(ts|tsx)\"",
    "dev:docker:nestjsapi:build": "bash -c \"export $(cat ./packages/api/env/env.dev | xargs) && docker compose up api --build\"",
    "dev:docker:nestjsapi": "bash -c \"export $(cat ./packages/api/env/env.dev | xargs) && docker compose up api\"",
    "dev:docker:down": "bash -c \"export $(cat ./packages/api/env/env.dev | xargs) && docker compose down\"",
    "dev:docker:kill": "bash -c \"export $(cat ./packages/api/env/env.dev | xargs) && docker compose kill\"",
    "api:dev": "bun run ./api/src/index.ts"
  },
  "dependencies": {
    "@nestjs/bullmq": "10.1.0",
    "@nestjs/common": "10.3.3",
    "@nestjs/swagger": "7.3.0",
    "@nestjs/config": "3.2.0",
    "@nestjs/core": "10.3.3",
    "@nestjs/jwt": "10.2.0",
    "postgres": "3.4.3",
    "i18next": "20.2.4",
    "aws4": "1.12.0",
    "runtypes": "6.7.0",
    "jsonwebtoken": "9.0.2",
    "redis": "4.6.13",
    "@socket.io/redis-adapter": "8.2.1",
    "socket.io": "4.7.2",
    "fluent-ffmpeg": "2.1.2",
    "@nestjs/platform-express": "10.3.3",
    "mime": "3.0.0",
    "bullmq": "5.4.0",
    "axios": "1.6.7",
    "@aws-sdk/client-s3": "3.525.0",
    "uuid": "9.0.0",
    "drizzle-orm": "0.29.4",
    "supertest": "6.3.4"
  },
  "devDependencies": {
    "@types/bun": "1.0.8",
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.3",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "drizzle-kit": "0.20.14",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.3",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-regexp": "2.2.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "prettier": "3.2.5"
  },
  "engines": {
    "node": "18"
  },
  "packageManager": "yarn@4.1.0"
}
