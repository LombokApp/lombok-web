{
  "name": "@lombokapp/sdk",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "version": "0.0.1-6",
  "type": "module",
  "exports": {
    "default": "./index.ts"
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node --format esm --external @lombokapp/types --external @lombokapp/auth-utils --external @lombokapp/utils",
    "build:clean": "yarn clean && yarn build",
    "clean": "rm -rf ./dist",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "tsc:check": "tsc --noEmit",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test:unit": "jest"
  },
  "devDependencies": {
    "@lombokapp/types": "workspace:*",
    "@lombokapp/auth-utils": "workspace:*",
    "@lombokapp/utils": "workspace:*",
    "prettier": "catalog:",
    "@types/bun": "catalog:",
    "eslint": "catalog:",
    "@eslint/js": "catalog:",
    "jiti": "catalog:",
    "typescript-eslint": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@stylistic/eslint-plugin-ts": "catalog:",
    "eslint-plugin-import": "catalog:",
    "eslint-plugin-eslint-comments": "catalog:",
    "eslint-plugin-promise": "catalog:",
    "eslint-plugin-simple-import-sort": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "globals": "catalog:"
  },
  "dependencies": {
    "openapi-fetch": "catalog:"
  }
}
