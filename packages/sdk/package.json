{
  "name": "@lombokapp/sdk",
  "main": "dist/index.js",
  "version": "0.0.1-6",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "rm -rf ./dist && NODE_ENV=production tsup src/index.ts --format esm,cjs --dts --external bun,@lombokapp/types,@lombokapp/auth-utils,@lombokapp/utils",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "tsc:check": "tsc --noEmit",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test:unit": "bun test"
  },
  "devDependencies": {
    "@lombokapp/types": "workspace:*",
    "@lombokapp/auth-utils": "workspace:*",
    "prettier": "catalog:",
    "@types/bun": "catalog:",
    "eslint": "catalog:",
    "@eslint/js": "catalog:",
    "jiti": "catalog:",
    "typescript-eslint": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@stylistic/eslint-plugin": "catalog:",
    "eslint-plugin-import": "catalog:",
    "eslint-plugin-eslint-comments": "catalog:",
    "eslint-plugin-promise": "catalog:",
    "eslint-plugin-simple-import-sort": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "globals": "catalog:",
    "tsup": "catalog:"
  },
  "dependencies": {
    "openapi-fetch": "catalog:"
  }
}
