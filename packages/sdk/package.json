{
  "name": "@lombokapp/sdk",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "version": "0.0.1-6",
  "type": "module",
  "exports": {
    "default": "./index.ts"
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node --format esm --external @lombokapp/types --external @lombokapp/auth-utils --external @lombokapp/utils",
    "build:clean": "yarn clean && yarn build",
    "clean": "rm -rf ./dist",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "tsc:check": "tsc --noEmit",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test:unit": "jest"
  },
  "devDependencies": {
    "@lombokapp/types": "workspace:*",
    "@lombokapp/auth-utils": "workspace:*",
    "@lombokapp/utils": "workspace:*",
    "prettier": "3.4.2",
    "tslib": "2.8.1"
  },
  "dependencies": {
    "openapi-fetch": "^0.14.0"
  }
}
