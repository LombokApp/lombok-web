{
  "name": "@stellariscloud/modules-worker-sdk",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "version": "0.0.1-6",
  "type": "module",
  "scripts": {
    "build": "(rm -rf ./*.tsbuildinfo ||  true) && yarn rollup -c rollup.config.js",
    "build:clean": "yarn clean && yarn workspaces @stellariscloud/api-client build:clean && yarn workspaces @stellariscloud/api-utils build:clean && yarn build",
    "publish:dev": "yarn build:clean && YARN_NPM_REGISTRY_SERVER=\"https://npm.wasteofpaper.com\" yarn npm publish --access restricted --tag @stellariscloud/modules-worker-sdk",
    "clean": "rm -rf ./dist",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "ts:check": "tsc --noEmit --project tsconfig.json",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test:unit": "jest"
  },
  "devDependencies": {
    "@jest/types": "29.6.1",
    "@rollup/plugin-alias": "5.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-typescript": "11.1.6",
    "@stellariscloud/api-client": "workspace:*",
    "@stellariscloud/api-utils": "workspace:*",
    "@stellariscloud/types": "workspace:*",
    "@types/jest": "29.5.3",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-regexp": "1.15.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.20.0",
    "jest": "29.6.2",
    "prettier": "2.8.8",
    "rollup": "4.12.0",
    "rollup-plugin-dts": "6.1.0",
    "ts-jest": "29.1.1",
    "tslib": "2.6.2",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "axios": "1.6.7"
  }
}
