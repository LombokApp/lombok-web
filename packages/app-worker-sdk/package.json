{
  "name": "@lombokapp/app-worker-sdk",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "version": "0.0.1-6",
  "type": "module",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node --format esm --external @lombokapp/types",
    "build:clean": "bun run clean && bun run workspaces @lombokapp/types build:clean && bun --cwd ./packages/utils build:clean && bun run build",
    "publish:dev": "bun run build:clean && YARN_NPM_REGISTRY_SERVER=\"https://npm.wasteofpaper.com\" yarn npm publish --access restricted --tag @lombokapp/app-worker-sdk",
    "clean": "rm -rf ./dist",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "tsc:check": "tsc --noEmit",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test:unit": "echo 'No tests'"
  },
  "devDependencies": {
    "@lombokapp/types": "workspace:*",
    "@types/bun": "catalog:",
    "prettier": "catalog:"
  },
  "dependencies": {
    "drizzle-orm": "catalog:",
    "socket.io-client": "catalog:",
    "zod": "catalog:"
  }
}
