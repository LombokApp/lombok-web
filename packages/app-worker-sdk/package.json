{
  "name": "@lombokapp/app-worker-sdk",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./drizzle": {
      "types": "./dist/drizzle.d.ts",
      "import": "./dist/drizzle.js",
      "default": "./dist/drizzle.js"
    },
    "./drizzle-pg-core": {
      "types": "./dist/drizzle-pg-core.d.ts",
      "import": "./dist/drizzle-pg-core.js",
      "default": "./dist/drizzle-pg-core.js"
    }
  },
  "version": "0.0.1-6",
  "type": "module",
  "scripts": {
    "build": "rm -rf ./dist && NODE_ENV=production tsup src/index.ts src/drizzle.ts src/drizzle-pg-core.ts --format esm,cjs --dts --external bun",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "tsc:check": "tsc --noEmit",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test:unit": "echo 'No tests'"
  },
  "devDependencies": {
    "@lombokapp/types": "workspace:*",
    "@types/bun": "catalog:",
    "eslint": "catalog:",
    "@eslint/js": "catalog:",
    "jiti": "catalog:",
    "typescript-eslint": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@stylistic/eslint-plugin": "catalog:",
    "eslint-plugin-import": "catalog:",
    "eslint-plugin-eslint-comments": "catalog:",
    "eslint-plugin-promise": "catalog:",
    "eslint-plugin-simple-import-sort": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "globals": "catalog:",
    "prettier": "catalog:",
    "tsup": "8.5.0"
  },
  "dependencies": {
    "drizzle-orm": "catalog:",
    "socket.io-client": "catalog:",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "drizzle-orm": "catalog:"
  }
}
