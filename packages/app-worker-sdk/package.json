{
  "name": "@lombokapp/app-worker-sdk",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "version": "0.0.1-6",
  "type": "module",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node --format esm --external @lombokapp/types",
    "build:clean": "bun run clean && bun run workspaces @lombokapp/types build:clean && bun --cwd ./packages/utils build:clean && bun run build",
    "publish:dev": "bun run build:clean && YARN_NPM_REGISTRY_SERVER=\"https://npm.wasteofpaper.com\" yarn npm publish --access restricted --tag @lombokapp/app-worker-sdk",
    "clean": "rm -rf ./dist",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "tsc:check": "tsc --noEmit",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test:unit": "echo 'No tests'"
  },
  "devDependencies": {
    "@lombokapp/types": "workspace:*",
    "@types/bun": "catalog:",
    "eslint": "catalog:",
    "@eslint/js": "catalog:",
    "jiti": "catalog:",
    "typescript-eslint": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@stylistic/eslint-plugin": "catalog:",
    "eslint-plugin-import": "catalog:",
    "eslint-plugin-eslint-comments": "catalog:",
    "eslint-plugin-promise": "catalog:",
    "eslint-plugin-simple-import-sort": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "globals": "catalog:",
    "prettier": "catalog:"
  },
  "dependencies": {
    "drizzle-orm": "catalog:",
    "socket.io-client": "catalog:",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "drizzle-orm": "catalog:"
  }
}
