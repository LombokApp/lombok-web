{
  "name": "@stellariscloud/api",
  "private": true,
  "files": [
    ".orm-cache/**",
    "dist/**",
    "locales/**",
    "mikro-orm.config.js"
  ],
  "main": "./src/index.ts",
  "publishConfig": {
    "main": "./dist/index.js",
    "scripts": {}
  },
  "scripts": {
    "build": "rm -rf .orm-cache && (rm -rf ./*.tsbuildinfo || true) && yarn tsc -p tsconfig-build.json",
    "clean": "rm -rf .orm-cache && rm -rf ./dist/*",
    "build:deps": "yarn workspace @stellariscloud/utils build:clean && yarn build:routes",
    "prepack": "yarn build",
    "db:init": "./cmd/db-init.sh",
    "db:migrate:format": "./cmd/db-migrate-format.sh",
    "db:migrate:new": "./cmd/db-migrate-new.sh",
    "db:migrate:up": "./cmd/db-migrate-up.sh",
    "db:migrate:down": "./cmd/db-migrate-down.sh",
    "db:reset": "./cmd/db-reset.sh",
    "db:reset:dev": "yarn db:reset && yarn db:seed:dev",
    "db:seed:dev": "./cmd/db-seed-dev.sh",
    "db:seed:staging": "./cmd/db-seed-staging.sh",
    "dev": "./cmd/dev.sh",
    "dev:docker": "bash -c \"export $(cat ./env/env.dev | xargs) && docker compose up\"",
    "dev:docker:clean": "(rm -rf ../../.docker/postgres/* || true) && (rm -rf ../../.docker/redis/* || true) && (rm -rf ../../.docker/minio || true)",
    "staging:docker:api": "bash -c \"export $(cat ./env/env.staging | xargs) && docker compose up api\"",
    "test:integration:docker:clean": "bash -c \"export $(cat ./env/env.test | xargs) && docker compose -f ../../docker-compose.test.yml down\"",
    "test:integration:docker": "bash -c \"export $(cat ./env/env.test | xargs) && docker compose -f ../../docker-compose.test.yml up\"",
    "dev:docker:api": "bash -c \"export $(cat ./env/env.dev | xargs) && docker compose up api\"",
    "dev:docker:db:up": "bash -c \"export $(cat ./env/env.dev | xargs) && docker compose up db\"",
    "dev:docker:services": "bash -c \"export $(cat ./env/env.dev | xargs) && docker compose up db redis arena swagger\"",
    "dev:docker:down": "bash -c \"export $(cat ./env/env.dev | xargs) && docker compose down\"",
    "dev:docker:kill": "bash -c \"export $(cat ./env/env.dev | xargs) && docker compose kill\"",
    "generate:routes-and-clients": "yarn generate:routes && yarn generate:clients",
    "generate:routes": "yarn tsoa spec-and-routes",
    "generate:clients": "yarn build && ./cmd/generate-clients.sh",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "orm": "./cmd/orm.sh",
    "prettier:check": "prettier --check . || { echo; echo 'run prettier:fix to fix this issue'; exit 1; }",
    "prettier:fix": "prettier --write .",
    "start": "./cmd/start.sh",
    "test:check": "./cmd/test-check.sh",
    "test:integration": "sh -c \"export $(cat ./env/env.test | xargs) && ./cmd/test-integration.sh\"",
    "test:unit": "./cmd/test-unit.sh",
    "ts:check": "tsc --noEmit --project tsconfig.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.342.0",
    "@aws-sdk/s3-request-presigner": "3.342.0",
    "@mikro-orm/cli": "5.7.10",
    "@mikro-orm/core": "5.7.10",
    "@mikro-orm/migrations": "5.7.10",
    "@mikro-orm/postgresql": "5.7.10",
    "@mikro-orm/reflection": "5.7.10",
    "@mikro-orm/sql-highlighter": "1.0.1",
    "@sentry/integrations": "6.3.6",
    "@sentry/node": "6.3.6",
    "@sentry/tracing": "6.3.6",
    "@stellariscloud/api-client": "workspace:*",
    "@stellariscloud/types": "workspace:*",
    "@stellariscloud/utils": "workspace:*",
    "@tsoa/runtime": "5.0.0",
    "ajv": "8.4.0",
    "ajv-formats": "2.1.0",
    "axios": "1.4.0",
    "bullmq": "1.80.3",
    "cors": "2.8.5",
    "express": "4.17.3",
    "express-openapi-validate": "0.6.1",
    "express-winston": "4.2.0",
    "glob": "7.1.7",
    "handy-redis": "2.2.1",
    "hashids": "2.2.8",
    "helmet": "5.0.2",
    "i18next": "20.2.4",
    "i18next-fs-backend": "1.1.4",
    "ioredis": "4.28.5",
    "jsonwebtoken": "8.5.1",
    "knex": "2.4.2",
    "logdna-winston": "3.0.5",
    "pg": "8.11.0",
    "prom-client": "14.0.1",
    "reflect-metadata": "0.1.13",
    "runtypes": "6.6.0",
    "tsoa": "5.1.1",
    "tsyringe": "4.7.0",
    "utility-types": "3.10.0",
    "uuid": "9.0.0",
    "winston": "3.6.0"
  },
  "devDependencies": {
    "@jest/types": "29.6.1",
    "@types/bull-arena": "3.0.7",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/express-winston": "4.0.0",
    "@types/glob": "8.1.0",
    "@types/helmet": "4.0.0",
    "@types/i18next-fs-backend": "1.0.0",
    "@types/jest": "29.5.3",
    "@types/jscodeshift": "0.11.0",
    "@types/jsonwebtoken": "9.0.2",
    "@types/mime-types": "2.1.1",
    "@types/pg-format": "1.0.1",
    "@types/sql-formatter": "2.3.0",
    "@types/supertest": "2.0.11",
    "@types/uuid": "9.0.1",
    "@types/workerpool": "6.1.0",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "env-cmd": "10.1.0",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-regexp": "1.15.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.20.0",
    "jest": "29.6.2",
    "jscodeshift": "0.12.0",
    "nodemon": "2.0.15",
    "prettier": "3.0.1",
    "pretty-format": "29.6.2",
    "sql-formatter": "4.0.2",
    "supertest": "6.3.3",
    "ts-it-types": "1.1.5",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.5",
    "winston-transport": "4.5.0"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/orm/orm-cli.config.ts"
    ]
  },
  "resolutions": {
    "node-fetch": "2.6.7"
  },
  "engines": {
    "node": "16"
  }
}
