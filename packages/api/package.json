{
  "name": "@stellariscloud/api",
  "private": true,
  "scripts": {
    "dev": "bun --watch --bun src/main.ts",
    "build": "nest build",
    "test": "jest --config ./jest.config.ts",
    "test:cov": "yarn test --coverage",
    "test:e2e:watch": "jest --config ./test/jest-e2e.config.ts --watchAll",
    "test:e2e": "jest --config ./test/jest-e2e.config.ts",
    "reset:sdk": "(rm -rf ./node_modules/@stellariscloud/app-worker-sdk || true)",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "db:migrate:new": "bunx drizzle-kit generate --config ./src/orm/drizzle.config.ts",
    "db:migrate:up": "bun --bun ./script/db-migrate-up.ts",
    "db:migrate:check": "./cmd/db-migrate-check.sh",
    "db:migrate:drop": "./cmd/db-migrate-drop.sh",
    "db:reset": "bun --bun ./script/db-reset.ts && bun --bun ./script/db-migrate-up.ts",
    "db:seed": "./cmd/db-seed.sh",
    "db:migrate:up:docker": "docker exec -it stellariscloud-api bun --bun --cwd ./packages/api db:migrate:up",
    "db:migrate:reset": "rm -rf ./src/orm/migrations/* && bun db:migrate:new",
    "db:seed:docker": "docker exec -it stellariscloud-api bun --bun --cwd ./packages/api db:seed",
    "db:reset:docker": "docker exec -it stellariscloud-api bun --bun --cwd ./packages/api db:reset",
    "db:reset:dev": "bun --bun db:reset:docker && bun --bun db:seed:docker",
    "dev:docker:api": "docker compose --env-file \"./.env\" up api",
    "dev:docker:services": "docker compose --env-file \"./.env\" up db",
    "dev:docker:e2e:services": "docker compose -f ../../docker-compose.test.yml --env-file \"./.env.test\" up dbtest miniotest minioclienttest",
    "dev:docker:e2e:down": "docker compose -f ../../docker-compose.test.yml --env-file \"./.env.test\" down",
    "dev:docker:e2e:watch": "docker compose -f ../../docker-compose.test.yml --env-file \"./.env.test\" up apitestwatch",
    "dev:docker:e2e:run": "docker compose -f ../../docker-compose.test.yml --env-file \"./.env.test\" up apitest",
    "metadata:generate": "bun --bun script/generate-metadata.ts",
    "api-client:generate": "./cmd/generate-api-client.sh",
    "prettier:check": "prettier --check \"./**/*.(ts|tsx)\"",
    "prettier:write": "prettier --write \"./**/*.(ts|tsx)\""
  },
  "dependencies": {
    "@anatine/zod-nestjs": "2.0.9",
    "@aws-sdk/client-s3": "3.609.0",
    "@heyputer/kv.js": "0.1.8",
    "@nestjs/common": "10.4.1",
    "@nestjs/config": "3.2.3",
    "@nestjs/core": "10.4.1",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/platform-express": "10.4.1",
    "@nestjs/platform-socket.io": "10.4.1",
    "@nestjs/swagger": "7.4.2",
    "@nestjs/websockets": "10.4.1",
    "@stellariscloud/core-worker": "workspace:*",
    "@stellariscloud/api-client": "workspace:*",
    "aws4": "1.13.0",
    "axios": "1.7.2",
    "cron": "3.1.7",
    "drizzle-orm": "0.36.0",
    "express": "4.19.2",
    "i18next": "23.11.5",
    "jsonwebtoken": "9.0.2",
    "mime": "4.0.4",
    "postgres": "3.4.4",
    "runtypes": "6.7.0",
    "rxjs": "7.8.1",
    "socket.io": "4.7.5",
    "supertest": "7.0.0",
    "utility-types": "3.11.0",
    "uuid": "10.0.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.3",
    "@nestjs/schematics": "10.1.3",
    "@nestjs/testing": "10.4.1",
    "@types/bun": "1.1.12",
    "@jest/types": "29.6.3",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "drizzle-kit": "0.27.1",
    "eslint": "9.6.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.6.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.4.0",
    "eslint-plugin-regexp": "2.6.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sonarjs": "1.0.3",
    "jest": "29.7.0",
    "prettier": "3.3.2",
    "ts-jest": "29.2.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0"
  }
}
