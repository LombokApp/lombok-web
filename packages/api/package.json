{
  "name": "@stellariscloud/api",
  "private": true,
  "scripts": {
    "dev": "bun --watch --bun src/main.ts",
    "build": "nest build",
    "test": "jest --config ./jest.config.unit.ts",
    "test:cov": "yarn test --coverage",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watchAll",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "reset:sdk": "(rm -rf ./node_modules/@stellariscloud/modules-worker-sdk || true)",
    "db:migrate:new": "bunx drizzle-kit generate:pg --config ./src/orm/drizzle.config.ts",
    "db:migrate:up": "bun --bun ./script/db-migrate-up.ts",
    "db:migrate:check": "./cmd/db-migrate-check.sh",
    "db:migrate:drop": "./cmd/db-migrate-drop.sh",
    "db:reset": "bun ./script/db-reset.ts && bun ./script/db-migrate-up.ts",
    "db:seed": "./cmd/db-seed.sh",
    "db:migrate:up:docker": "docker exec -it stellariscloud-api bun --cwd ./packages/api db:migrate:up",
    "db:migrate:reset": "rm -rf ./src/orm/migrations/* && bun db:migrate:new",
    "db:seed:docker": "docker exec -it stellariscloud-api bun --bun --cwd ./packages/api db:seed",
    "db:reset:docker": "docker exec -it stellariscloud-api bun --bun --cwd ./packages/api db:reset",
    "db:reset:dev": "bun db:reset:docker && bun db:seed:docker",
    "dev:docker:api": "docker compose --env-file \"./.env\" up api",
    "dev:docker:services": "docker compose --env-file \"./.env\" up db redis swagger",
    "dev:docker:e2e:db": "docker compose -f ../../docker-compose.test.yml --env-file \"./.env.test\" up dbtest",
    "dev:docker:e2e:watch": "docker compose -f ../../docker-compose.test.yml --env-file \"./.env.test\" up apitestwatch",
    "dev:docker:e2e:run": "docker compose -f ../../docker-compose.test.yml --env-file \"./.env.test\" up apitest",
    "metadata:generate": "rm -f ./src/nestjs-metadata.ts && npx ts-node -r tsconfig-paths/register script/generate-metadata.ts",
    "api-client:generate": "./cmd/generate-api-client.sh",
    "prettier:check": "prettier --check \"./**/*.(ts|tsx)\"",
    "prettier:write": "prettier --write \"./**/*.(ts|tsx)\""
  },
  "dependencies": {
    "@anatine/zod-nestjs": "2.0.7",
    "@aws-sdk/client-s3": "3.525.0",
    "@nestjs/bullmq": "10.1.0",
    "@nestjs/common": "10.3.5",
    "@nestjs/config": "3.2.0",
    "@nestjs/core": "10.3.5",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/platform-express": "10.3.5",
    "@nestjs/swagger": "7.3.0",
    "@socket.io/redis-adapter": "8.2.1",
    "aws4": "1.12.0",
    "axios": "1.6.7",
    "bullmq": "5.4.1",
    "drizzle-orm": "0.29.4",
    "express": "4.18.3",
    "fluent-ffmpeg": "2.1.2",
    "i18next": "20.2.4",
    "jsonwebtoken": "9.0.2",
    "mime": "3.0.0",
    "postgres": "3.4.3",
    "redis": "4.6.13",
    "runtypes": "6.7.0",
    "socket.io": "4.7.2",
    "supertest": "6.3.4",
    "uuid": "9.0.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.3",
    "@types/bun": "1.0.10",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "drizzle-kit": "0.20.14",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.3",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-regexp": "2.2.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0"
  }
}
