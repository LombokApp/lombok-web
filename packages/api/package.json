{
  "name": "@stellariscloud/api",
  "private": true,
  "files": [
    ".orm-cache/**",
    "dist/**",
    "locales/**"
  ],
  "main": "./src/index.ts",
  "publishConfig": {
    "main": "./dist/index.js",
    "scripts": {}
  },
  "scripts": {
    "build": "rm -rf .orm-cache && (rm -rf ./*.tsbuildinfo || true) && yarn tsc -p tsconfig-build.json",
    "clean": "rm -rf .orm-cache && rm -rf ./dist/*",
    "build:deps": "yarn workspace @stellariscloud/utils build:clean && yarn build:routes",
    "prepack": "yarn build",
    "db:init": "./cmd/db-init.sh",
    "db:migrate:up": "./cmd/db-migrate-up.sh",
    "db:migrate:check": "./cmd/db-migrate-check.sh",
    "db:migrate:drop": "./cmd/db-migrate-drop.sh",
    "db:reset": "./cmd/db-reset.sh",
    "db:migrate-new:dev": "./cmd/db-migrate-new.sh",
    "db:migrate-up:dev": "docker exec -it stellariscloud-api yarn workspace @stellariscloud/api db:migrate:up",
    "db:reset:dev": "docker exec -it stellariscloud-api yarn workspace @stellariscloud/api db:reset",
    "dev": "./cmd/dev.sh",
    "test:integration:docker:clean": "bash -c \"export $(cat ./env/env.test | xargs) && docker compose -f ../../docker-compose.test.yml down\"",
    "test:integration:docker": "bash -c \"export $(cat ./env/env.test | xargs) && docker compose -f ../../docker-compose.test.yml up\"",
    "generate:routes-and-clients": "yarn generate:routes && yarn generate:clients",
    "generate:routes": "yarn tsoa spec-and-routes",
    "generate:clients": "yarn build && ./cmd/generate-clients.sh",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "orm": "./cmd/orm.sh",
    "prettier:check": "prettier --check . || { echo; echo 'run prettier:fix to fix this issue'; exit 1; }",
    "prettier:fix": "prettier --write .",
    "start": "./cmd/start.sh",
    "test:check": "./cmd/test-check.sh",
    "test:integration": "sh -c \"export $(cat ./env/env.test | xargs) && ./cmd/test-integration.sh\"",
    "test:unit": "./cmd/test-unit.sh",
    "ts:check": "tsc --noEmit --project tsconfig.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.342.0",
    "@sendgrid/mail": "7.7.0",
    "@sentry/integrations": "6.3.6",
    "@sentry/node": "6.3.6",
    "@sentry/tracing": "6.3.6",
    "@socket.io/redis-adapter": "8.2.1",
    "@stellariscloud/types": "workspace:*",
    "@stellariscloud/utils": "workspace:*",
    "@stellariscloud/workers": "workspace:*",
    "@tsoa/runtime": "5.0.0",
    "ajv": "8.4.0",
    "ajv-formats": "2.1.0",
    "aws4": "1.12.0",
    "axios": "1.4.0",
    "bufferutil": "4.0.7",
    "bullmq": "4.8.0",
    "cors": "2.8.5",
    "drizzle-orm": "0.28.6",
    "express": "4.17.3",
    "express-openapi-validate": "0.6.1",
    "express-winston": "4.2.0",
    "glob": "7.1.7",
    "helmet": "5.0.2",
    "i18next": "20.2.4",
    "i18next-fs-backend": "1.1.4",
    "jsonwebtoken": "8.5.1",
    "logdna-winston": "3.0.5",
    "mime": "3.0.0",
    "pg": "8.11.0",
    "postgres": "3.4.0",
    "redis": "4.6.10",
    "reflect-metadata": "0.1.13",
    "runtypes": "6.7.0",
    "socket.io": "4.7.2",
    "tsoa": "5.1.1",
    "tsyringe": "4.8.0",
    "utf-8-validate": "6.0.3",
    "utility-types": "3.10.0",
    "uuid": "9.0.0",
    "winston": "3.6.0"
  },
  "devDependencies": {
    "@esbuild/linux-arm64": "0.18.20",
    "@jest/types": "29.6.1",
    "@stellariscloud/api-client": "workspace:*",
    "@types/aws4": "1.11.3",
    "@types/bull-arena": "3.0.7",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/glob": "8.1.0",
    "@types/i18next-fs-backend": "1.0.0",
    "@types/jest": "29.5.3",
    "@types/jscodeshift": "0.11.0",
    "@types/jsonwebtoken": "9.0.2",
    "@types/mime": "3.0.1",
    "@types/pg-format": "1.0.1",
    "@types/redis": "4.0.11",
    "@types/sql-formatter": "2.3.0",
    "@types/supertest": "2.0.11",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "drizzle-kit": "0.19.13",
    "env-cmd": "10.1.0",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-regexp": "1.15.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.20.0",
    "jest": "29.6.2",
    "jscodeshift": "0.12.0",
    "nodemon": "2.0.15",
    "prettier": "2.8.8",
    "sql-formatter": "4.0.2",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.0.2",
    "winston-transport": "4.5.0"
  },
  "engines": {
    "node": "18"
  }
}
