  "dependencies": {
    "@lombokapp/app-browser-sdk": "workspace:*",
    "@lombokapp/ui-toolkit": "workspace:*",
    "@lombokapp/utils": "workspace:*",
    "@tanstack/react-query": "^5.83.0",
    "openapi-react-query": "^0.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/react": "18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "globals": "^15.14.0",
    "tailwindcss": "^3.4.17",
    "vite": "^7.1.9"
