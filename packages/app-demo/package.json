{
  "name": "@lombokapp/app-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "app-demo-ui",
    "app-demo-workers"
  ],
  "scripts": {
    "package": "bun --cwd app-demo-ui package && bun --cwd app-demo-workers package && cp config.json ../../apps/demo/config.json && cp .publicKey ../../apps/demo/.publicKey",
    "lint:check": "bun --cwd app-demo-ui lint:check && bun --cwd app-demo-workers lint:check"
  },
  "devDependencies": {
    "eslint": "catalog:",
    "@eslint/js": "catalog:",
    "jiti": "catalog:",
    "typescript-eslint": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@stylistic/eslint-plugin-ts": "catalog:",
    "eslint-plugin-import": "catalog:",
    "eslint-plugin-eslint-comments": "catalog:",
    "eslint-plugin-promise": "catalog:",
    "eslint-plugin-simple-import-sort": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "globals": "catalog:"
  }
}