{
  "name": "@lombokapp/app-browser-sdk",
  "version": "0.0.1",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "rm -rf ./dist && NODE_ENV=production tsup src/index.ts --format esm,cjs --dts --external bun,@lombokapp/auth-utils,@lombokapp/types,@lombokapp/sdk",
    "dev": "bun --watch src/index.ts",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "tsc:check": "tsc --noEmit",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "dependencies": {
    "@lombokapp/sdk": "workspace:*",
    "@lombokapp/types": "workspace:*",
    "@lombokapp/auth-utils": "workspace:*",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "devDependencies": {
    "@types/bun": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@eslint/js": "catalog:",
    "eslint": "catalog:",
    "jiti": "catalog:",
    "typescript-eslint": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@stylistic/eslint-plugin": "catalog:",
    "eslint-plugin-import": "catalog:",
    "eslint-plugin-eslint-comments": "catalog:",
    "eslint-plugin-promise": "catalog:",
    "eslint-plugin-simple-import-sort": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "prettier": "catalog:",
    "globals": "catalog:",
    "tsup": "catalog:"
  }
}
