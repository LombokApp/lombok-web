    "eslint-plugin-regexp": "2.7.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sonarjs": "3.0.1",
    "jest": "29.7.0",
    "prettier": "3.4.2",
    "rollup": "4.30.1",
    "rollup-plugin-dts": "6.1.1",
